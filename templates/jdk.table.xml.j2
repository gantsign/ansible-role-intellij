<application>
  <component name="ProjectJdkTable">
    <jdk version="2">
      <name value="default" />
      <type value="JavaSDK" />
      <version value="java version &quot;{{ java_version_result.stdout }}&quot;" />
      <homePath value="{{ intellij_default_jdk_home }}" />
      <roots>
        <annotationsPath>
          <root type="composite">
            <root type="simple" url="jar://$APPLICATION_HOME_DIR$/lib/jdkAnnotations.jar!/" />
          </root>
        </annotationsPath>
        <classPath>
          <root type="composite">
            {% for path_entry in intellij_default_jdk_classpath %}
            <root type="simple" url="jar://{{ intellij_default_jdk_home }}/{{ path_entry }}!/" />
            {% endfor %}
          </root>
        </classPath>
        <javadocPath>
          <root type="composite" />
        </javadocPath>
        <sourcePath>
          <root type="composite">
            <root type="simple" url="jar://{{ intellij_default_jdk_home }}/src.zip!/" />
            <root type="simple" url="jar://{{ intellij_default_jdk_home }}/javafx-src.zip!/" />
          </root>
        </sourcePath>
      </roots>
      <additional />
    </jdk>
  </component>
</application>
